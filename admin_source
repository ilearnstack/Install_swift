#Please create a source file , let's named if as "admin_source"
#includes following contents 

export ST_AUTH=http://localhost:8080/auth/v1.0
export ST_USER=admin:admin
export ST_KEY=admin



#Now source those variables into env.

# source admin_source


#You can access your swift env now
# swift stat
# swift list
# swift upload 
# swift -h

